{% if action_params %}
[global]
ioengine={{ action_params.ioengine }}
iodepth={{ action_params.iodepth }}
direct=1
rw={{ action_params.operation }}
bs={{ action_params.block_size }}
numjobs={{ action_params.num_jobs }}
group_reporting=1
runtime=30
{% for dev in action_params.disk_devices %}
[job {{loop.index}}]
filename={{dev}}
{% endfor %}
{% endif %}
